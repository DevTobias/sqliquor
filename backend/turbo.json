{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build", "init"]
    },
    "init": { "dependsOn": ["^build"] },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build", "init"]
    },
    "lint": {}
  }
}

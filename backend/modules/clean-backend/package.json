{
  "name": "@modules/clean-backend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rm -r dist && (yarn build-esm & yarn build-cjs & tsc)",
    "build-esm": "esbuild src/index.ts --bundle --minify --target=es2022 --format=esm --outfile=dist/index.mjs --external:fastify",
    "build-cjs": "esbuild src/index.ts --bundle --minify --target=es2022 --format=cjs --outfile=dist/index.cjs --external:fastify"
  },
  "dependencies": {
    "fastify": "^4.18.0"
  },
  "devDependencies": {
    "@modules/eslint-config": "^1.0.0",
    "@modules/prettier-config": "^1.0.0",
    "esbuild": "^0.18.1",
    "typescript": "^5.1.3"
  },
  "prettier": "@modules/prettier-config",
  "eslintConfig": {
    "extends": [
      "@modules/eslint-config"
    ]
  }
}

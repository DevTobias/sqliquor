{
  "name": "@modules/clean-backend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rimraf dist && concurrently \"yarn build-esm\" \"yarn build-cjs\" \"tsc\"",
    "build-esm": "esbuild src/index.ts --bundle --minify --format=esm --outfile=dist/index.mjs --external:fastify",
    "build-cjs": "esbuild src/index.ts --bundle --minify --format=cjs --outfile=dist/index.cjs --external:fastify"
  },
  "dependencies": {
    "fastify": "^4.17.0"
  },
  "devDependencies": {
    "@modules/eslint-config": "^1.0.0",
    "@modules/prettier-config": "^1.0.0",
    "concurrently": "^8.1.0",
    "esbuild": "^0.17.4",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  },
  "prettier": "@modules/prettier-config",
  "eslintConfig": {
    "extends": [
      "@modules/eslint-config"
    ]
  }
}
